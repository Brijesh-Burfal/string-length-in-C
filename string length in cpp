#include<iostream>
using namespace std;
int main()
{
string str ="hello world";

int len=str.size();
int len1=str.length();
 int count=0;
  for(int i = 0; str[i] != '\0'; i++){
      count++;
   }

cout<<"string length by size fn " <<len<<"\n";
cout<<"string length by length fn " <<len1<<"\n";
cout<<"string length by loop " <<count<<"\n";

}
